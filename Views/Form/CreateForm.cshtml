@model labr10.Models.FliedDataModel

<form method="post" asp-controller="Form" asp-action="Luck">
    <div>
        <label asp-for="FullName"></label>
        <input asp-for="FullName" />
        <span asp-validation-for="FullName"></span>
    </div>

    <div>
        <label asp-for="Email"></label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>

    <div>
        <label asp-for="Date"></label>
        <input asp-for="Date" type="date" />
        <span asp-validation-for="Date"></span>
    </div>
    <div>
        <label asp-for="Product"></label>
        <select asp-for="Product">
            <option value="JavaScript">JavaScript</option>
            <option value="C#">C#</option>
            <option value="Java">Java</option>
            <option value="Python">Python</option>
            <option value="Основи">Основи</option>
        </select>
        <span asp-validation-for="Product"></span>
    </div>

    <button type="submit">Зареєструватися</button>
</form>